program example
type
	w1 = record
		integer x;
		integer y;
	end;
var
	integer a;
	integer b;
	w1 c;
procedure sd(integer s1; var integer s2);
	var
		integer ss;
	procedure sss(integer a);
		begin
			write(a)
		end
	begin
		if s1<10 then
			s2 := s1+10
		else
			s2 := s1-10
		fi;
		sss(s2)
	end
begin
	read(a);
	sd(a, b);
	write(b)
end.